{
    "name": "crossy-road",
    "version": "1.0.0",
    "description": "Crossy Road Clone",
    "main": "main.js",
    "scripts": {
        "pack": "electron-builder --dir",
        "dist": "electron-builder --publish never",
        "start": "electron ."
    },
    "build": {
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thesourorange/neilsextra/crossy-road"
    },
    "keywords": [
        "Electron",
        "Crossy Road",
        "demo"
    ],
    "author": "GitHub",
    "license": "CC0-1.0",
    "dependencies": {
        "three.js": "^0.77.1"
    },
    "devDependencies": {
        "electron": "^14.0.1",
        "electron-builder": "^22.11.7",
        "node": "^16.9.1"
    },
    "bugs": {
        "url": "https://github.com/thesourorange/neilsextra/crossy-road"
    },
    "homepage": "https://www.brittliff.org"
}
